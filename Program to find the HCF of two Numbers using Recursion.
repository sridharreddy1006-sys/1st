#include <stdio.h>
int findHCF(int a, int b) {
    if (b == 0) {
        return a;
    }
    else {
        return findHCF(b, a % b);
    }
}
int main() {
    int num1, num2, hcf;
    printf("Enter two positive integers: ");
    scanf("%d %d", &num1, &num2);
    hcf = findHCF(num1, num2);
    printf("HCF of %d and %d is: %d\n", num1, num2, hcf);
    return 0;
}

output:
Enter two positive integers: 7
9
HCF of 7 and 9 is: 1
