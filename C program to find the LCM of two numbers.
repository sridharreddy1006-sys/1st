#include <stdio.h>
int main() {
    int a, b, num1, num2, temp, gcd, lcm;
    printf("Enter two integers: ");
    if (scanf("%d %d", &num1, &num2) != 2) {
        printf("Invalid input.\n");
        return 1;
    }
    a = num1;
    b = num2;
    while (num2 != 0) {
        temp = num2;
        num2 = num1 % num2;
        num1 = temp;
    }
    gcd = num1;
    lcm = (a * b) / gcd;
    printf("The GCD is: %d\n", gcd);
    printf("The LCM of %d and %d is: %d\n", a, b, lcm);
    return 0;
}

output:
Enter two integers: 5
10
The GCD is: 5
The LCM of 5 and 10 is: 10
