#include <stdio.h>
int main() {
    int n,cnt = 0;
    printf("n:");
    scanf("%d",&n);
    if (n <= 1)
        printf("%d is NOT prime\n", n);
    else {
        for (int i = 1; i <= n; i++) {
            if (n % i == 0)
                cnt++;
        }
        if (cnt > 2)
            printf("%d is NOT prime\n", n);
        else
            printf("%d is prime", n);
    }

    return 0;
}

output:
n:89
89 is prime
