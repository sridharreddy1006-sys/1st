#include <stdio.h>
int main()
{
    int a,b,c;
    printf("enter three numbers:\n"); 
    scanf("%d%d%d", &a, &b, &c);
    if (a > b && a > c)
        printf("large no.%d", a);
    else if (b > a && b > c)
        printf("large no.%d", b);
    else
        printf("large no.%d", c);
    return 0;
}

output:
enter three numbers:
11
14
13
large no.14
