#include <stdio.h>
int main() {
  int length,width,area;
  printf("enter length:");
  scanf("%d",&length);
  printf("enter width:");
  scanf("%d",&width);
  area = length * width;
  printf("Area of the rectangle is: %d", area);
  return 0;
}

output:
enter length:8
enter width:4
Area of the rectangle is: 32
